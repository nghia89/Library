<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css">
    @Styles.Render("~/Content/css")
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <div class="contain @(Request.Cookies["menu"] != null ?Request.Cookies["menu"].Value:"")" id="contain">
        @Html.Partial("layout/header")
        @Html.Partial("layout/menuleft")
        <div class="right">
            <div class="content">
                <div class="clearfix layout-content">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")

    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />
    <script src="~/app/AngularChartJs/jquery.min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    @*<script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>*@

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js"></script>
    @*<script src="~/Scripts/angular.min.js"></script>
        <script src="~/Scripts/angularjs.min.js"></script>
        <script src="~/Scripts/LibraryAngularJS.js"></script>*@
    <link href="~/Content/PagedList.css" rel="stylesheet" />


    @Scripts.Render("~/bundles/main")
    @RenderSection("scripts", required: false)

</body>
</html>
