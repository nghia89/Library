@model BiTech.Library.Models.TrangThaiSachViewModels
@{
    ViewBag.Title = "ThemTT";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}
<div class="Title">Thêm Trạng Thái</div>
<div class="container clearfix">


    <div class="container">
        @using (Html.BeginForm("Them", "TrangThaiSach", FormMethod.Post, new { role = "form" }))
        {

            <div class="form-group row">
                <label class="col-form-label col-sm-2" for="tensach">Trạng Thái:</label>
                <div class="col-sm-9">
                    @Html.TextBoxFor(m => m.TenTT, new { @class = "form-control", @id = "tentt" })
                    @Html.ValidationMessageFor(m => m.TenTT, "", new { @class = "text-danger" })
                </div>

            </div>
            <div class="form-group row">
                <label class="col-form-label col-sm-2"></label>
                <div class="col-sm-9">
                    <input type="submit" class="btn btn-primary" name="btnThemTrangThai" value="Thêm Mới" />
                    <input type="button" onclick="goBack()" class="btn btn-default" value="Trở lại" />
                </div>

            </div>

        }
    </div>
</div>

