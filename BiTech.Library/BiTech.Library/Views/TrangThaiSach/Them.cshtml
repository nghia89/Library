@model BiTech.Library.Models.TrangThaiSachViewModels
@{
    ViewBag.Title = "ThemTT";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}
<div class="container clearfix">
    <h2 style="">Thêm Trạng Thái</h2>

    <div class="container">
        @using (Html.BeginForm("Them", "TrangThaiSach", FormMethod.Post, new { role = "form" }))
        {
            <div class="form-group row">
                <label>Tên trang thái</label>
                <div  class="col-sm-10">
                    <input type="text" class="input-group-text" name="TenTT" />
                </div>
            </div>

            <div class="form-group row">
                <input type="submit" name="btnThemTrangThai" value="Thêm Mới" />
            </div>
            
        }
    </div>
</div>

